<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.app.controller.srorage.EditStorageOrDeliveryController"
      stylesheets="/style/edit.css" styleClass="v-box"
      prefHeight="300.0" prefWidth="600.0" alignment="CENTER">
    <Label fx:id="baseText" text="Редактирование склада" textAlignment="CENTER">
        <VBox.margin>
            <Insets top="20" bottom="30"/>
        </VBox.margin>
    </Label>
    <ComboBox fx:id="boxItems" prefHeight="45.0" minWidth="350.0" maxWidth="400" promptText="Выберите склад">
        <VBox.margin>
            <Insets bottom="20"/>
        </VBox.margin>
    </ComboBox>
    <TextField fx:id="name" prefHeight="45.0" minWidth="350.0" maxWidth="400" promptText="Название">
        <VBox.margin>
            <Insets bottom="20"/>
        </VBox.margin>
    </TextField>
    <HBox>
        <Button prefHeight="45.0" onAction="#cansel" prefWidth="200.0" text="Отмена">
            <HBox.margin>
                <Insets bottom="40" top="20" left="30"/>
            </HBox.margin>
        </Button>
        <Button prefHeight="45.0" prefWidth="200.0" text="Принять" onAction="#close">
            <HBox.margin>
                <Insets bottom="40" top="20" left="140"/>
            </HBox.margin>
        </Button>
    </HBox>
</VBox>
